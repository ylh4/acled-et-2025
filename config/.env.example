# ACLED API Credentials
# Copy this file to .env and fill in your actual credentials
# Do NOT commit .env to version control

# Your ACLED account email (used for myACLED login)
ACLED_USERNAME=your_email@example.com

# Your ACLED account password
ACLED_PASSWORD=your_password_here

# ACLED OAuth Client ID (default is 'acled')
ACLED_CLIENT_ID=acled

# ACLED API Base URL
ACLED_BASE_URL=https://acleddata.com/api/
